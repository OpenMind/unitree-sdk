          ".*/navigate_to_pose",
          ".*/follow_waypoints",
          ".*/navigate_through_poses",
          ".*/compute_path_to_pose",
          ".*/compute_path_through_poses",
          ".*/follow_path",
          ".*/spin",
          ".*/wait",
          ".*/backup",
          ".*/drive_on_heading",
          ".*/assisted_teleop",
        ],
        action_clients: [
          ".*/navigate_to_pose",
          ".*/follow_waypoints",
          ".*/navigate_through_poses",
          ".*/compute_path_to_pose",
          ".*/compute_path_through_poses",
          ".*/follow_path",
          ".*/spin",
          ".*/wait",
          ".*/backup",
          ".*/drive_on_heading",
          ".*/assisted_teleop"
        ],
      },
      queries_timeout: {
        default: 10.0,
        actions: {
          send_goal: 1.0,
          cancel_goal: 1.0,
          get_result: [
            ".*long_mission=3600",
            ".*short_action=10.0",
            ".*navigation=120.0",
            ".*mapping=60.0",
            ],
        }
      }
